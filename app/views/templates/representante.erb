<div id="representante-{{id}}" class="representante actor-sumario {{camara}} {{partido}} clearfix">
	<a href="/actores/{{stub}}/{{id}}" title="Detalles de {{nombre}}">
		<span class="camara">{{camara}}</span>
		<div class="headshot-container">
			<div class="headshot {{partido}}">
				<!-- /foto-de/{{stubImagen}}.jpg -->
				<div class="headshot-image" style="background-image:url(/actores/imagen/{{imagen}});"></div>
			</div>
		</div>
		<div class="representante-sumario">
			<p class="byline">
				{{distrito}}
			</p>
			
			<h2>{{nombre}}</h2>
			<span class="partido">{{partido}}</span>
			{{#eleccion}}
			<span class="eleccion">({{eleccion}})</span>
			{{/eleccion}}
		</div>
		<div class="acciones-contacto">
			<span class="icono detalles">next</span>
		</div>
	</a>
</div>